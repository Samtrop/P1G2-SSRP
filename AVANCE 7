<script>
//Codigo de programacion - INICIO
//CONDICIONAL COMPUESTA EJSERCICIO

//CONDICIONAL ANIDADA EJERCICIO
function CondicionalAnidada_UsandoO(){
    if(5>=5){
        if(9<10){
            if()
        }
    }
}
//CONDICIONALES COMPUESTAS ||
function CondicionalAnidada_UsandoO(){
    if((5>9) ||(7!=5)){
        alert("TRUE")
    }else{
        alert("FALSE")
    }
}

//CONDICONALES COMPUESTAS
function CondicionalCompuesta_UsandoY(){
    if((5>9)&&(7!=5)){
        alert("true")
    }else{
        alert("FALSE")
    }
}
//CONDICIONALES ANIDADAS
function CondicionalAnidada_UsandoY(){
    if(5>9){
        if(7!=5){
            alert("TRUE")
        }else{
            alert("FALSE")
        }
    }else{
        alert("FALSE")
    }
}
function EjercicioUno(){
    if(((5 >= 5) != (9 < 10)) || ((10 > 10) == false)){
        alert("TRUE")
    }else{
        alert("FALSE")

    }

}
function EjercicioDos(){
    if(((9 < 5) == true ) || (! false || (5 == 9 ))){
      alert("TRUE")    
    }else{
        alert("FALSE")
    }
}
function EjercicioTres(){
    if((((7 >= 7) == true) || (false == true))|| !(7 > 7)){
        alert("TRUE")
    }else{
        alert("FALSE")
    }
}
function EjercicioCuatro(){
    if((((true || ! false) && (false)) != ((false || false != (true))))){
        alert("TRUE")
    }else{
        alert("FALSE")
    }
}
    
     
    

    


    





function SumarFactorialAyB(){
    var numeroA = 5 //mensaje = 120
    var numeroB = 5 //mensaje = 120
    
    var resultadoA = FactorialNumero(numeroA)
    var resultadoB = FactorialNumero(numeroB)

    var resultadoFinal = resultadoA + resultadoB
    alert(resultadoFinal)//mensaje = 240
}

function FactorialNumero(numero){
    //no hay (var numero) porque es un parametro de la function
     var i = 1
     var resultado = 1
     while(i <= numero){
         resultado = resultado * i
         i = i + 1   
     } 
alert(resultado)
return resultado//NUEVO: La funcion ahora nretorna un valor     
}
//Codigo de programacion - FIN
